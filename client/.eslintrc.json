{
	"$schema": "https://cdn.jsdelivr.net/npm/@eslint/eslintrc/schemas/eslintrc-schema.json",
	"ignorePatterns": ["src/components/ui/**"],
	"extends": [
		"next/core-web-vitals",
		"standard",
		"plugin:tailwindcss/recommended",
		"prettier"
	],
	"rules": {
		"camelcase": "off",
		"no-unused-vars": "off",
		"tailwindcss/no-custom-classname": "off"
	}
}
